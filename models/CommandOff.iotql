--
--  compile to JSON with 'iotql-model Command.iotql'
--
CREATE MODEL CommandOff WITH
    schema:name = "Command Off"
ATTRIBUTE off WITH
    schema:name = "command",
    iot:purpose = iot-purpose:on.false,
    iot:type = iot:type.null,
    iot:write = true,
    iot:read = false,
    iot:actuator = true,
    iot:sensor = false
;
