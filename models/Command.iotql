--
--  compile to JSON with 'iotql-model Command.iotql'
--
CREATE MODEL Command WITH
    schema:name = "Command",
    schema:description = "Command"
ATTRIBUTE on WITH
    schema:name = "command",
    iot:purpose = iot-purpose:command,
    iot:type = iot:type.null,
    iot:write = true,
    iot:read = false,
    iot:actuator = true,
    iot:sensor = false
;
